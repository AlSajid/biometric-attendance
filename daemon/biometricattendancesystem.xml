<service>
	<id>biometricattendancesystem.exe</id>
	<name>Biometric Attendance System</name>
	<description>Biometric Attendance System</description>
	<executable>C:\Program Files\nodejs\node.exe</executable>
	<argument>--harmony</argument>
	<argument>D:\projects\biometric-attendance\node_modules\node-windows\lib\wrapper.js</argument>
	<argument>--file</argument>
	<argument>D:\projects\biometric-attendance\service.js</argument>
	<argument>--scriptoptions=</argument>
	<argument>--log</argument>
	<argument>Biometric Attendance System wrapper</argument>
	<argument>--grow</argument>
	<argument>0.25</argument>
	<argument>--wait</argument>
	<argument>1</argument>
	<argument>--maxrestarts</argument>
	<argument>3</argument>
	<argument>--abortonerror</argument>
	<argument>n</argument>
	<argument>--stopparentfirst</argument>
	<argument>undefined</argument>
	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>BLACKHOLE-DCL</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>D:\projects\biometric-attendance</workingdirectory>
</service>